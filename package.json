    "babel-eslint": "^10.0.1",
    "ink": "^2.0.6",
    "ink-tab": "^2.0.1",
    "ink-text-input": "^3.1.0",
    "ink-testing-library": "^1.0.1",
    "inquirer": "^6.2.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.6",
    "window": "^4.2.6"
